{
    "version": 2,
    "functions": {
        "public/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        {
            "src": "/(css|js|fonts|images|svg|favicon.ico)(.*)",
            "dest": "public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.php"
        }
    ]
} 